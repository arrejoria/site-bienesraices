    "postcss-cli": "^11.0.0",
    "sequelize-cli": "^6.6.2",
    "tailwindcss": "^3.4.9",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bootstrap-icons": "^1.11.3",
    "cookie-parser": "^1.4.6",
